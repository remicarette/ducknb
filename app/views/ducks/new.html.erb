<h1>Ducks#new</h1>
<div class= "container">
  <div>
      <%= simple_form_for @duck do |f| %>
      <%= f.input :name %>
      <%= f.input :birthdate %>
      <%= f.input :race %>
      <%= f.input :sex %>
      <%= f.input :weight %>
      <%= f.input :colour %>
      <%= f.input :tags %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit "new duck", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
